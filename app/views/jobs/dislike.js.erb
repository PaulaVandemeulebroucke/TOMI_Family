function refreshCard(outerHTML) {
  const job = document.getElementById(<%= @job.id %>);
  job.outerHTML = outerHTML;
}

refreshCard('<%= j render "jobs/card", job:@job %>');

function refreshCard2(outerHTML) {
  const jobb = document.getElementById(<%= @jobb.id %>);
  jobb.outerHTML = outerHTML;
}

refreshCard2('<%= j render "jobs/card2", jobb:@jobb %>');
