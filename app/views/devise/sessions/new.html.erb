<div id="students-content">
  <div class="back">
    <i class=" back fas fa-chevron-left" onclick="history.back(-1)"></i>
  </div>
  <div class="container-signupstudents">
    <div class="congtainer-logo-signup">
      <%= image_tag("logo-entier.png", class: "logo2") %>
    </div>
<!--     <h1 class="title-students1">Bienvenue dans la </h1>
    <h1 class="title-students2">T O M I - <em>Family</em></h1> -->
    <p class="subtitle-students">Welcome back ! ðŸ˜Ž</p>
  </div>

  <div class="container-signupstudents">
    <div class="row">
      <div class="col-sm-6 col-sm-offset-3">

      <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="form-inputs">
          <%= f.input :email, required: false, autofocus: true %>
          <%= f.input :password, required: false %>
          <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
        </div>

        <div class="form-actions">
          <%= f.button :submit, "Log in" %>
        </div>
      <% end %>

      <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
</div>

