<div class="wrapper">
  <div class="banner">
    <%= render '/shared/banner' %>
    <div class="sign-in">
      <% if user_signed_in? %>
        <!-- Links when logged in -->
        <%= link_to "Voir ma session", homepage_path %>
      <% else %>
        <!-- Login link (when logged out) -->
        <%= link_to t(".sign_in", default: "Login"), new_user_session_path %>
      <% end %>
    </div>
    <%= image_tag("logo.png", class: "logo")  %>
  </div>
  <div class="gradient">
  </div>
  <video autoplay loop class="fillWidth">
    <source src='<%= asset_path "jeunes.mp4"%>' type="video/mp4"/>
    Your browser does not support the video tag. I suggest you upgrade your browser.
    <!-- <source src="PATH_TO_WEBM" type="video/webm" />Your browser does not support the video tag. I suggest you upgrade your browser. -->
